// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

// schema.prisma
// schema.prisma

model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name     String
  dob      DateTime
  gender   String
  email    String   @unique
  phone    String?
  company  String?
  password String
}
